<div class="row">
    <div class="col-sm-6 col-md-3" ng-repeat="product in searchResults">
        <div class="thumbnail custom">
            <img src="../../../images/ProductImages/Apples/Fuji_Apple.jpg" alt="Fuji Apple"style="max-width: 100px; max-height: 100px; display: block; margin-left: auto; margin-right: auto; border-style: none">
            <div class="caption">
                <h3>{{product.products.productprice}}</h3>
                <p>{{product.productname}}</p>
                <p>{{product.description}} </p>
                <p>
                    <a href="#" class="btn btn-default" role="button" ng-click="addToCart(product)">Add to Cart</a>
                </p>
            </div>
        </div>
    </div>
</div>

