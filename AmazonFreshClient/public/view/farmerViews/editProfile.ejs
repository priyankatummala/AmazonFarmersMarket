<div style="margin: 0 auto;  width: 50%">
    <form role="form" name="form" novalidate>
        <div class="form-group">
            <label>Farmer ID/SSN</label>
            <input type="text" class="form-control" id="editFarmerID" ng-model="editFarmerID" readonly>
        </div>
        <div class="form-group">
            <label>First Name</label>
            <input type="text" class="form-control" id="editFirstname" ng-model="editFirstname" ng-model-options="{ updateOn: 'blur' }" required>
        </div>
        <div class="form-group">
            <label>Last Name</label>
            <input type="text" class="form-control" id="editLastname" ng-model="editlastname" required>
        </div>
        <div class="form-group">
            <label>Address</label>
            <input type="text" class="form-control" id="address" ng-model="form.address" required>
        </div>
        <div class="form-group">
            <label>city</label>
            <input type="email" class="form-control" id="city" ng-model="form.city" required>
        </div>
        <div class="form-group">
            <label>State</label>
            <input type="text" class="form-control" id="state" ng-model="form.state" required>
        </div>
        <div class="form-group">
            <label>Zipcode</label>
            <input type="text" class="form-control" id="zipcode" ng-model="form.zipcode" ng-pattern="/^(\d{5}-\d{4}|\d{5})$/" required>
        </div>
        <div class="form-group">
            <label>Phone</label>
            <input type="text" class="form-control" id="phonenumber" ng-model="form.phonenumber" ng-pattern="myRegex" required>
        </div>


        <button type="submit" class="btn btn-success" ng-click="editFarmerProfile()">Update</button>

    </form>
</div>