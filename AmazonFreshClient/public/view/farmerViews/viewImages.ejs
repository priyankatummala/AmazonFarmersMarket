<div>
    <div ng-model="farmerImage">
        <img ng-src="{{image}}" alt="Farmer Image"
             style="max-width: 100px; max-height: 100px; display: block; margin-left: auto; margin-right: auto; border-style: none">
    </div>
    <div ng-model="farmerVideo">
        <video ng-src="{{video}}" width="640" height="480" controls>
            <source type="video/mp4">
        </video>
    </div>
</div>