<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.26/angular-route.js"></script>
    <link rel="stylesheet" type="text/css" href="/stylesheets/customer/customerHomeStyles.css">
    <script src="/javascripts/angular/customers/customerHomeController.js"></script>
</head>
<body ng-app="customerApp">
<div ng-controller="ParentController">
    <div style="background-image: url(/images/header/taskbar-bg.png); width: 100%; height: 45px" class="container-fluid">
        <img src="/images/header/af-logo-white.png" style="margin-top: 5px">
    </div>

    <div id="shopBar">
        <div class="leftNav">
            <div id="shopButton" class=""></div>
        </div>
        <form novalidate>
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label" for="sm"></label>
                <div class="col-sm-8">
                    <input class="form-control" type="text" name="searchItem" ng-model="searchItem"
                           placeholder="Search All Amazon">
                <span style="float: right;margin: -32px -1px;">
                    <button type="submit" ng-click="search();" class="goButton"></button>
                </span>
                </div>
            </div>
        </form>
    </div>

    <a href="#/editProfile">Your Profile</a>
    <a href="#/cartDetails">My cart</a>

    <div ng-view class="container-fluid"></div>
</div>
</body>
</html>